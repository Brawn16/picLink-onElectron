{
  "name": "piclink",
  "author": "piclink",
  "description": "photography-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "license": "MIT",
  "main": "public/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "prod": "build --em.main=build/main.js",
    "release": "build --em.main=build/main.js --publish=always",
    "preprod": "npm run build",
    "prerelease": "npm run build"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-packager": "^12.1.0",
    "electron-updater": "^2.19.1",
    "firebase": "^5.0.4",
    "lodash": "^4.17.4",
    "node-notifier": "^5.2.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-firebase-file-uploader": "^2.4.1"
  },
  "devDependencies": {
    "concurrently": "3.5.1",
    "electron": "^2.0.2",
    "electron-builder": "^19.54.0",
    "prettier": "^1.10.2",
    "react-scripts": "0.9.2",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "io.example.picLink-onElectron",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
  }
}